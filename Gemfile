## Para usar local descomente as linhas abaixo

# gem "jekyll", "~> 4.3.3"
# gem "minima", "~> 2.5"
# gem "minimal-mistakes-jekyll"
# gem 'jekyll-paginate'
# group :jekyll_plugins do
#   gem "jekyll-feed", "~> 0.12"
# end

## Para usar no github descomente as linhas abaixo e comente as linhas acima

source "https://rubygems.org"
gem "github-pages", group: :jekyll_plugins
gem "jekyll-include-cache", group: :jekyll_plugins
gem "webrick", "~> 1.8"